{
  "log": {
    "level": "fatal"
  },
  "dns": {
    "servers": [
      {
        "tag": "remote",
        "address": "https://8.8.8.8/dns-query",
        "address_resolver": "local"
      },
      {
        "tag": "local",
        "address": "https://223.5.5.5/dns-query",
        "detour": "direct"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "local"
      }
    ]
  },
  "inbounds": [
    {
      "type": "mixed",
      "listen": "::",
      "listen_port": 7890,
      "sniff": true,
      "set_system_proxy": true
    }
  ],
  "outbounds": [
    {
      "type": "selector",
      "tag": "default",
      "outbounds": [
        "auto",
        "manual",
        "direct",
        "block"
      ],
      "interrupt_exist_connections": true
    },
    {
      "type": "urltest",
      "tag": "auto",
      "outbounds": [],
      "url": "https://nyaa.si/robots.txt",
      "interval": "30m0s"
    },
    {
      "type": "selector",
      "tag": "manual",
      "outbounds": [],
      "interrupt_exist_connections": true
    },
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "block",
      "tag": "block"
    },
    {
      "type": "dns",
      "tag": "dns"
    }
  ],
  "route": {
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
