      {
        "rule_set": [
          "geoip-malware",
          "geoip-phishing",
          "geosite-category-ads-all",
          "geosite-cryptominers",
          "geosite-malware",
          "geosite-phishing"
        ],
        "outbound": "block"
      },
      {
        "rule_set": [
          "direct",
          "geoip-cn",
          "geosite-cn",
          "geosite-connectivity-check",
          "geosite-private"
        ],
        "outbound": "direct"
      }
    ],
    "rule_set": [
      {
        "type": "remote",
