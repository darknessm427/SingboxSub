        ],
        "type": "logical"
      },
      {
        "outbound": "block",
        "rule_set": [
          "geoip-malware",
          "geoip-phishing",
          "geosite-category-ads-all",
          "geosite-cryptominers",
          "geosite-malware",
          "geosite-nsfw",
          "geosite-phishing"
        ]
      },
      {
        "outbound": "direct",
        "rule_set": [
          "geoip-ru",
          "geosite-category-ru"
        ]
      }
    ]
  }
