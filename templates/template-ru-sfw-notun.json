    ],
    "rules": [
      {
        "outbound": "dns",
        "protocol": "dns"
      },
      {
        "mode": "or",
        "outbound": "direct",
        "rules": [
          {
            "ip_is_private": true
          },
          {
            "port": [
              123,
              4460
            ]
          },
          {
            "rule_set": "geosite-private"
          },
          {
            "process_name": [
              "conjure-client",
              "conjure-client.exe",
              "i2p",
              "i2p.exe",
              "i2pd",
              "i2pd.exe",
              "lyrebird",
