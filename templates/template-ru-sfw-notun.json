{
  "dns": {
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "independent_cache": true,
    "rules": [
      {
        "outbound": "any",
        "server": "direct"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "fakeip"
      }
    ],
    "servers": [
      {
        "address": "https://8.8.8.8/dns-query",
        "address_resolver": "direct",
        "tag": "remote"
      },
      {
        "address": "local",
        "detour": "direct",
        "tag": "direct"
      },
      {
        "address": "fakeip",
        "tag": "fakeip"
      }
    ]
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": true
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "yacd",
      "external_ui_download_detour": "direct"
    }
  },
  "inbounds": [
    {
      "listen": "::",
      "listen_port": 7890,
      "set_system_proxy": true,
      "sniff": true,
      "tag": "mixed",
      "type": "mixed"
    }
  ],
  "log": {
    "level": "fatal"
  },
  "ntp": {
    "detour": "direct",
    "enabled": false,
    "server": "2.pool.ntp.org",
    "server_port": 123
  },
  "outbounds": [
    {
      "interrupt_exist_connections": true,
      "outbounds": [
        "auto",
        "manual",
        "direct",
        "block"
      ],
      "tag": "default",
      "type": "selector"
    },
    {
      "interval": "30m0s",
      "outbounds": [],
      "tag": "auto",
      "type": "urltest",
      "url": "https://www.youtube.com/generate_204"
    },
    {
      "interrupt_exist_connections": true,
      "outbounds": [],
      "tag": "manual",
      "type": "selector"
    },
    {
      "domain_strategy": "prefer_ipv6",
      "tag": "direct",
      "type": "direct"
    },
    {
      "tag": "block",
      "type": "block"
    },
    {
      "tag": "dns",
      "type": "dns"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "rule_set": [
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geosite-category-ru",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geosite-category-ru.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
