{
  "dns": {
    "rules": [
      {
        "outbound": "any",
        "server": "local"
      }
    ],
    "servers": [
      {
        "address": "https://8.8.8.8/dns-query",
        "address_resolver": "local",
        "tag": "remote"
      },
      {
        "address": "8.8.8.8",
        "detour": "direct",
        "tag": "local"
      }
    ]
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": true
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "yacd",
      "external_ui_download_detour": "direct"
    }
  },
  "inbounds": [
    {
      "auto_route": true,
      "exclude_package": [
        "com.android.captiveportallogin",
        "net.i2p.android.router",
        "org.onionshare.android",
        "org.onionshare.android.fdroid",
        "org.openobservatory.ooniprobe",
        "org.purplei2p.i2pd",
        "org.torproject.android",
        "org.torproject.torbrowser",
        "org.torproject.torbrowser_alpha",
        "pan.alexander.tordnscrypt",
        "pan.alexander.tordnscrypt.gp",
        "pan.alexander.tordnscrypt.stable"
      ],
      "inet4_address": "172.19.0.1/30",
      "inet4_route_exclude_address": "192.168.0.0/16",
      "inet6_address": "fdfe:dcba:9876::1/126",
      "inet6_route_exclude_address": [
        "fc00::/7",
        "fe80::/10"
      ],
      "sniff": true,
      "strict_route": true,
      "type": "tun"
    },
    {
      "listen": "::",
      "listen_port": 7890,
      "sniff": true,
      "type": "mixed"
    }
  ],
  "log": {
    "level": "fatal"
  },
  "outbounds": [
    {
      "interrupt_exist_connections": true,
      "outbounds": [
        "auto",
        "manual",
        "direct",
        "block"
      ],
      "tag": "default",
      "type": "selector"
    },
    {
      "interval": "30m",
      "outbounds": [],
      "tag": "auto",
      "type": "urltest",
      "url": "https://yts.mx/robots.txt"
    },
    {
      "interrupt_exist_connections": true,
      "outbounds": [],
      "tag": "manual",
      "type": "selector"
    },
    {
      "tag": "direct",
      "type": "direct"
    },
    {
      "tag": "block",
      "type": "block"
    },
    {
      "tag": "dns",
      "type": "dns"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "rule_set": [
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geosite-ir",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geosite-ir.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geoip-ir",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geoip-ir.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geosite-category-ads-all",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geosite-category-ads-all.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geosite-connectivity-check",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geosite-connectivity-check.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geosite-cryptominers",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geosite-cryptominers.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geosite-malware",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geosite-malware.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geosite-phishing",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geosite-phishing.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geosite-private",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geosite-private.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geoip-malware",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geoip-malware.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geoip-phishing",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geoip-phishing.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "direct",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/demarcush/breakfree@master/direct.srs"
      }
    ],
    "rules": [
      {
        "outbound": "dns",
        "protocol": "dns"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "outbound": "block",
        "rule_set": [
          "geoip-malware",
          "geoip-phishing",
          "geosite-category-ads-all",
          "geosite-cryptominers",
          "geosite-malware",
          "geosite-phishing"
        ]
      },
      {
        "outbound": "direct",
        "rule_set": [
          "direct",
          "geoip-ir",
          "geosite-ir",
          "geosite-connectivity-check",
          "geosite-private"
        ]
      }
    ]
  }
}
