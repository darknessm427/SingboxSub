    }
  ],
  "subscriptions": [
    {
      "name": "azadi",
      "url": "https://cdn.jsdelivr.net/gh/azadiazinjamigzare/Service@main/Sub",
      "update_interval": "1m0s"
    },
    {
      "name": "ircp",
      "url": "https://cdn.jsdelivr.net/gh/IranianCypherpunks/SingBox@main/Sub",
      "update_interval": "1m0s"
    },
    {
      "name": "warp",
      "url": "https://itshin.serv00.net/api/warpinator/",
      "update_interval": "1m0s"
    },
    {
      "name": "collector",
      "url": "https://cdn.jsdelivr.net/gh/Mahdi0024/ProxyCollector@master/sub/singbox.json",
      "update_interval": "1m0s"
    },
    {
      "name": "sub-web",
      "url": "https://sub.xeton.dev/sub?target=singbox&url=https://cdn.jsdelivr.net/gh/itsyebekhe/HiN-VPN@main/subscription/base64/mix|https://cdn.jsdelivr.net/gh/roosterkid/openproxylist@main/V2RAY_BASE64.txt|https://cdn.jsdelivr.net/gh/Kwinshadow/TelegramV2rayCollector@main/sublinks/b64mix.txt&list=true&fdn=true&sort=true",
      "update_interval": "1m0s"
    },
    {
      "name": "eternity",
      "url": "https://cdn.jsdelivr.net/gh/mahdibland/V2RayAggregator@master/Eternity.yml",
      "update_interval": "1m0s"
    },
    {
      "name": "final",
      "url": "http://127.0.0.1:8080/servers",
      "update_interval": "1m0s",
      "process": {
        "filter_type": [
          "http",
          "shadowsocksr",
          "socks"
        ],
        "remove": true,
        "rewrite_multiplex": {
          "enabled": true,
          "padding": true
        }
      },
      "deduplication": true
    }
  ],
  "templates": [
    {
