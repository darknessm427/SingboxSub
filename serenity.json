    }
  ],
  "profiles": [
    {
      "name": "servers",
      "template": "breakfree",
      "subscription": [
        "azadi",
        //"ircp",
        "warp",
        "collector",
        "sub-web",
        "eternity"
      ]
    },
    {
      "name": "final",
      "template": "breakfree",
      "subscription": "final"
    }
  ]
}
