{
  "log": {
    "level": "debug"
  },
  "listen": "127.0.0.1:8080",
  "cache_file": ".github/serenity.db",
  "outbounds": [
    {
      "type": "direct"
    },
    {
      "type": "dns"
    }
  ],
  "subscriptions": [
    {
      "name": "collector",
      "url": "https://cdn.jsdelivr.net/gh/Mahdi0024/ProxyCollector@master/sub/singbox.json"
    },
    {
      "name": "eternity",
      "url": "https://cdn.jsdelivr.net/gh/mahdibland/V2RayAggregator@master/Eternity.yml"
    },
    {
      "name": "azadi",
      "url": "https://cdn.jsdelivr.net/gh/azadiazinjamigzare/Service@main/Sub"
    },
    {
      "name": "ircp",
      "url": "https://cdn.jsdelivr.net/gh/IranianCypherpunks/SingBox@main/Sub"
    },
    {
      "name": "jnir",
      "url": "https://cdn.jsdelivr.net/gh/bamdad23/JavidnamanIran@main/JNIR"
    },
    {
      "name": "final",
      "url": "http://127.0.0.1:8080/servers",
      "update_interval": "5m0s",
      "process": {
        "filter_type": [
          "http",
          "shadowsocksr",
          "socks"
        ],
        "remove": true,
        "rewrite_multiplex": {
          "enabled": true,
          "padding": true
        }
      },
      "deduplication": true
    }
  ],
  "templates": [
    {
      "name": "breakfree"
    }
  ],
  "profiles": [
    {
      "name": "servers",
      "template": "breakfree",
      "subscription": [
        "collector",
        "eternity",
        "azadi",
        //"ircp",
        "jnir"
      ]
    },
    {
      "name": "final",
      "template": "breakfree",
      "subscription": "final"
    }
  ]
}
